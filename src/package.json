{
  "name": "levarne_ecercise-1",
  "version": "1.0.0",
  "description": "1st exercise for LeVarne",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass css/style.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass css/style.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WiliTheSParrow/LeVarne_Exercise-1"
  },
  "author": "Julia Wilhelm",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^7.1.0"
  }
}
